<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans>
		<form-bean name="RF" type="beans.AadharForm"/>
	</form-beans>
	<action-mappings>
		<action path="/reg" name="RF" input="/index.jsp"/>
		<action path="/changelang" type="beans.ChangeLanguageController">
		<forward name="index" path="/index.jsp"></forward>
		</action>
	</action-mappings>
	<message-resources parameter="resources.AadharForm"></message-resources>
</struts-config>